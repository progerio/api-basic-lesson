#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/paulo/Documentos/Projects/api-basic-lesson/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.26.7_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@29.7.0_hai7bumuwwa7yrzdhnlffxe4ii/node_modules/ts-jest/node_modules:/home/paulo/Documentos/Projects/api-basic-lesson/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.26.7_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@29.7.0_hai7bumuwwa7yrzdhnlffxe4ii/node_modules:/home/paulo/Documentos/Projects/api-basic-lesson/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/paulo/Documentos/Projects/api-basic-lesson/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.26.7_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@29.7.0_hai7bumuwwa7yrzdhnlffxe4ii/node_modules/ts-jest/node_modules:/home/paulo/Documentos/Projects/api-basic-lesson/node_modules/.pnpm/ts-jest@29.2.5_@babel+core@7.26.7_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@29.7.0_hai7bumuwwa7yrzdhnlffxe4ii/node_modules:/home/paulo/Documentos/Projects/api-basic-lesson/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
