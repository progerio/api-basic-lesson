{
  "compilerOptions": {
    "target": "es2016",                                  
    "module": "Node16",                                
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "outDir": "build",
    "types": ["jest", "node"],
  }
  ,"exclude": ["node_modules/"],
  "include": ["src/**/*.ts", "test/**/*.ts"]
}
